    "start": "next start",
    "prettier": "prettier src/**/*.{ts,tsx} --write",
    "eslint": "eslint --ext .ts,.tsx --fix .",
    "delete:nostr-relaypool-ts": "rimraf ./node_modules/nostr-relaypool/*.ts",
